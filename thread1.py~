from threading import Thread

def threadOne():
	for x in range(1000000):
		i = i + 1

def threadTwo():
	for y in range(1000000):
		i = i - 1

global i

def main():
	i = 0
	thread_1 = Thread(target = threadOne, args = (),)
	thread_2 = Thread(target = threadTwo, args = (),)

	thread_1.start()
	thread_2.start()

	thread_1.join()
	thread_2.join()
	
	print i

main()
